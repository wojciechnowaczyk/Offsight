


<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Offsight{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/AboutPage.css') }}" />
        <link href='https://fonts.googleapis.com/css?family=Oswald' rel='stylesheet'>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD7A9RN0emXJmj7OJjZG3IzxVPGXACcSRs&callback=initMap" type="text/javascript"></script>

    </head>
    <body>
        <div class="InfoBox">
          <div class="HomeIcon">
            <a href="{{path('home')}}"><img src='/img/home.png' /></a>
          </div>
          <div class="ContactIcon">
            <a href="{{path('onas')}}"><img src='/img/contact.png' /></a>
          </div>
        </div>
        <div class="PoznanHeader">
          <a href={{path('home')}}><img src="/img/header.png" class="HeaderImage" /></a>
        </div>
        <div class="FirstPerson">
          <div class="FirstLeft">
            <div>
              <p class="Name">
              Jan Radomski
            </p>
          </div>
            <div class="SocialNav">

            <ul class="social-nav">

           <li><a href="https://www.facebook.com/jan.radomski.7?fref=ts" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"/></a></li>

           <li><a href="https://twitter.com/jwmrad" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"></a></td></a></li>

            <li><a href="https://www.instagram.com/jwmrad/" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"></a></li>

            </ul>
          </div>


          </div>
          <div class="FirstRight">
            <img class="PersonalPhotos" src="/img/radomski.jpg" class="HeaderImage" />
          </div>
        </div>
        <div class="SecondPerson">
          <div class="SecondLeft">
            <img class="PersonalPhotos" src="/img/nowaczyk.jpg" class="HeaderImage" />
          </div>
          <div class="SecondRight">
            <p class="Name">
              Wojciech Nowaczyk
            </p>
            <div class="SocialNav">

            <ul class="social-nav">

           <li><a href="https://www.facebook.com/wojciech.nowaczyk.5" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"/></a></li>

           <li><a href="https://twitter.com/w_nowaczyk" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"></a></td></a></li>

            <li><a href="https://www.instagram.com/wojciech.nowaczyk/" target="_blank"><img class="social-media-icon" src="data:image/png;base64,"></a></li>

            </ul>
          </div>
          </div>
        </div>
        <div class="ContactBox">
          <h1 class="ContactUsHeader">SKONTAKTUJ SIĘ Z NAMI</h1>
          <div id="ContactForm" >
            {{ form_start(form) }}
            {{ form_widget(form.email, {'attr': {'placeholder': 'Adres e-mail'} }) }}
            {{ form_widget(form.name, {'attr': {'placeholder': 'Imię i nazwisko'} }) }}
            {{ form_widget(form.message, {'attr': {'placeholder': 'Wiadomość'} }) }}
            {{ form_end(form) }}
          </div>
        </div>


        {% block javascripts %}{% endblock %}
    </body>
</html>
